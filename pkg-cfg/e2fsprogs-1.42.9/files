# vim: syntax=sh
etc/mke2fs.conf
sbin/badblocks
if {CONFIG_E2FSPROGS_HAS_BLKID:-} == y
sbin/blkid
fi
sbin/e2fsck
if {CONFIG_E2FSPROGS_HAS_FINDFS:-} == y
sbin/findfs
fi
sbin/fsck
sbin/fsck.ext2
sbin/fsck.ext3
sbin/fsck.ext4
sbin/mke2fs
sbin/mkfs.ext2
sbin/mkfs.ext3
sbin/mkfs.ext4
sbin/tune2fs
usr/sbin/mklost+found
